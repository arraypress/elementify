{
  "name": "arraypress/elementify",
  "version": "1.0.0",
  "description": "A fluent interface for generating HTML elements in WordPress",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/elementify/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "arraypress/wp-composer-assets": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Elementify\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/elementify/issues",
    "source": "https://github.com/arraypress/elementify"
  },
  "keywords": [
    "wordpress",
    "html",
    "elements",
    "components",
    "ui",
    "interface",
    "builder",
    "fluent-interface",
    "forms",
    "cards",
    "modals",
    "notices",
    "tabs",
    "accordions",
    "breadcrumbs",
    "progress-bar",
    "status-badge"
  ]
}